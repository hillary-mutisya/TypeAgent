{
  "name": "markdown-agent",
  "version": "0.0.1",
  "description": "Markdown agent",
  "homepage": "https://github.com/microsoft/TypeAgent#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/TypeAgent.git",
    "directory": "ts/packages/agents/markdown"
  },
  "license": "MIT",
  "author": "Microsoft",
  "type": "module",
  "exports": {
    "./agent/manifest": "./src/agent/markdownManifest.json",
    "./agent/handlers": "./dist/agent/markdownActionHandler.js"
  },
  "scripts": {
    "build": "npm run tsc && npm run build:frontend",
    "build:frontend": "vite build",
    "postbuild": "copyfiles -u 1 \"src/**/config.json\" dist",
    "clean": "rimraf --glob dist *.tsbuildinfo *.done.build.log",
    "dev": "node scripts/dev-viewer.js",
    "dev:hmr": "node scripts/dev-viewer-hmr.js",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run tsc && node scripts/dev-viewer.js",
    "dev:frontend": "vite --host",
    "prettier": "prettier --check . --ignore-path ../../../.prettierignore",
    "prettier:fix": "prettier --write . --ignore-path ../../../.prettierignore",
    "tsc": "tsc -b"
  },
  "dependencies": {
    "@milkdown/core": "^7.3.6",
    "@milkdown/crepe": "^7.3.6",
    "@milkdown/plugin-history": "^7.3.6",
    "@milkdown/plugin-math": "^7.3.6",
    "@milkdown/preset-commonmark": "^7.3.6",
    "@milkdown/preset-gfm": "^7.3.6",
    "@milkdown/theme-nord": "^7.3.6",
    "@milkdown/utils": "^7.3.6",
    "@typeagent/agent-sdk": "workspace:*",
    "aiclient": "workspace:*",
    "common-utils": "workspace:*",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "katex": "^0.16.21",
    "markdown-it": "^14.1.0",
    "markdown-it-texmath": "^1.0.0",
    "mermaid": "^10.6.1",
    "typeagent": "workspace:*",
    "typechat": "^0.1.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.7",
    "@types/katex": "^0.16.7",
    "@types/markdown-it": "^14.1.2",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "~5.4.5",
    "vite": "^4.5.0"
  }
}
